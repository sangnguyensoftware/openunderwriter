            </list-property>
            <xml-property name="queryText"><![CDATA[select 
	vehRiskCodeIDrsk, 
	clmAmountPaidAmount ,
	polGrossPremiumAmount,
	(coalesce ( clmAmountPaidAmount, 0) / polGrossPremiumAmount) * 100 as ratio
	 
from polPolicy 
	left outer join clmClaim on polID = clmPolicyIDpol
	join vehVehicle on clmVehicleIDveh = vehId
	
where polReasonIDrea not like 'ENDORSEMENT'
	and polInception >= ?
	and polInception <= ?
	
group by vehRiskCodeIDrsk]]></xml-property>
