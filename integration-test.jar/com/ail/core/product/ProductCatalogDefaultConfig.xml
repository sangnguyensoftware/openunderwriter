               xsi:noNamespaceSchemaLocation="http://www.appliedindustriallogic.com/schemas/Configuration-1.0.xsd">
    <name>ProductCatalog configuration</name>
    <timeout>60000</timeout>
    <who>factory</who>
    <version>$Revision: 1.2 $</version>
    <manager>com.ail.core.product.listproducts.ListProductsService</manager>
    
    <group name="products">
        <parameter name="com.ail.core.product.TestProduct01"/>
