   <xs:complexType name="AccountType">
      <xs:attribute name="number" type="xs:string" use="optional"/>
      <xs:attribute name="ref" type="xs:string" use="optional"/>
      <xs:attribute name="name" type="xs:string" use="optional"/>
      <xs:attribute name="sortCode" type="xs:string" use="required"/>
      <xs:attribute name="type" type="xs:string" use="optional"/>
      <xs:attribute name="bankName" type="xs:string" use="optional"/>
      <xs:attribute name="branchName" type="xs:string" use="optional"/>
   </xs:complexType>
   <xs:attributeGroup name="QuantityAndMonetaryValueType">
      <xs:attribute name="numberOf" type="xs:string" use="required"/>
      <xs:attribute name="currency" type="CurrencyType" use="optional"/>
      <xs:attribute name="valueOf" type="ValueType" use="required"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="MonetaryValueType">
      <xs:attribute name="currency" type="CurrencyType"/>
      <xs:attribute name="valueOf" type="ValueType"/>
   </xs:attributeGroup>
   <xs:simpleType name="TransactionCodeType">
      <xs:annotation>
         <xs:documentation xml:lang="en">Defines constraints of a transaction code</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:decimal">
         <xs:totalDigits value="2"/>
         <xs:fractionDigits value="0" fixed="true"/>
      </xs:restriction>
   </xs:simpleType>
   <!-- Defines valid values for a value type - revisit -->
   <xs:simpleType name="ValueType">
      <xs:annotation>
         <xs:documentation xml:lang="en">Defines a monetary value</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:decimal">
         <xs:totalDigits value="13"/>
         <xs:fractionDigits value="2" fixed="true"/>
      </xs:restriction>
   </xs:simpleType>
