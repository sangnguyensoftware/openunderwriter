            <xs:extension base="PageElement">
                <xs:attribute name="type" use="optional" type="xs:string" />
                <xs:attribute name="addAndDeleteEnabled" use="optional" type="xs:boolean" />
                <xs:attribute name="minRows" use="optional" type="xs:nonNegativeInteger" />
                <xs:attribute name="maxRows" use="optional" type="xs:positiveInteger" />
                <xs:attribute name="repeatedTitle" use="optional" type="xs:string" />
