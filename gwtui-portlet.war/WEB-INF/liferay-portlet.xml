<liferay-portlet-app>
	<portlet>
		<portlet-name>quicksearch</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>/quicksearch/quicksearch.nocache.js</header-portlet-javascript>
		<css-class-wrapper>quicksearch-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>policysearch</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>/policysearch/policysearch.nocache.js</header-portlet-javascript>
		<css-class-wrapper>policysearch-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
