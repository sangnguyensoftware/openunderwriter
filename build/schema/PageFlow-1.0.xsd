                <xs:attribute name="hideReferrals" use="optional" type="xs:boolean" />
                <xs:attribute name="hideDeclines" use="optional" type="xs:boolean" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AnswerSection">
        <xs:annotation>
            <xs:documentation>Deprecated. Use PageSection instead.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PageContainer" />
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AssessmentSheetDetails">
