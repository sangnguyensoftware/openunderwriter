        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PageElement">
                <xs:sequence>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="question" type="Question" />
                        <xs:element name="questionSeparator" type="QuestionSeparator" />
                        <xs:element name="questionWithSubSection" type="QuestionWithSubSection" />
                        <xs:element name="questionWithDetails" type="QuestionWithDetails" />
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
