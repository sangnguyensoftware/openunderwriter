        <parameter name="com.ail.core.product.TestProduct03"/>
        <parameter name="com.ail.core.product.TestProduct04"/>
        <parameter name="com.ail.core.product.TestProduct05"/>
        <parameter name="com.ail.core.product.TestProduct06"/>
        <parameter name="com.ail.core.product.TestProduct07"/>
        <parameter name="com.ail.core.product.TestProduct08"/>
        <parameter name="com.ail.core.product.TestProduct09"/>
        <parameter name="com.ail.core.product.TestProduct10"/>
        <parameter name="com.ail.core.product.TestProduct11"/>
        <parameter name="com.ail.core.product.TestProduct12"/>
    </group>
</configuration>
